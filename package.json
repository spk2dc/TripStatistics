{
  "name": "herokutripstatistics",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "build": "cd tripstatistics && yarn build",
    "install-client": "cd tripstatistics && yarn install",
    "heroku-postbuild": "yarn install-client && yarn build",
    "start": "cd api_backend && python server.py",
    "client": "cd tripstatistics && yarn start",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"nodemon dist/server.js\" \"yarn client\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.6.0",
    "yarn": "1.22.4"
  },
  "proxy": "http://localhost:8000"
}
